  440  cat > lightning_studio/on_start.sh <<'EOF'\n#!/usr/bin/env bash\nset -euo pipefail\necho "[on_start] starting at $(date)"\n# persistent kernel dir in your project (already used earlier)\nPERSISTENT_KERNEL_DIR="/teamspace/studios/this_studio/.jupyter_kernels/share/jupyter"\n\n# Ensure the persistent kernel dir exists\nmkdir -p "${PERSISTENT_KERNEL_DIR}/kernels"\n\n# Add it to JUPYTER_PATH for this session so the Jupyter server sees it\nexport JUPYTER_PATH="${PERSISTENT_KERNEL_DIR}:${JUPYTER_PATH:-}"\n# Install (or re-install) the kernelspec into the persistent folder\n# Use --prefix so we don't touch system dirs; --no-deps not relevant here.\n# Run with the python from the env you want (adjust path if needed)\n"/teamspace/studios/this_studio/micromamba/envs/unsloth/bin/python" -m ipykernel install \\n  --prefix="/teamspace/studios/this_studio/.jupyter_kernels" \\n  --name ml \\n  --display-name "ML (python 3.10)" || true\n# Make a user-local link so servers that ignore JUPYTER_PATH still see it.\n# ~/.local is ephemeral, but this script runs at every studio start, so it's OK to recreate.\nmkdir -p "${HOME}/.local/share/jupyter/kernels"\nln -sf "${PERSISTENT_KERNEL_DIR}/kernels/ml" "${HOME}/.local/share/jupyter/kernels/ml"\n\necho "[on_start] done at $(date)"\nEOF
